

<div class="container">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
        <%= form_for(@article) do |f| %>
          <div class="form-group">
            <%= f.label :title %>
            <%= f.text_field :title%>
          </div>
          <div class="form-group">
          <%= f.label :content %>
          <%= f.text_field :content %>
          </div>
          <%= f.submit %>
        <% end %>
    </div>
  </div>
</div>
