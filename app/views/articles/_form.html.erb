

<div class="container">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <div class="form">
        <%= form_for(@article) do |f| %>
          <%= f.label :title %>
          <%= f.text_field :title %>

          <%= f.label :content %>
          <%= f.text_field :content %>

          <%= f.submit %>
        <% end %>
      </div>
    </div>
  </div>
</div>
