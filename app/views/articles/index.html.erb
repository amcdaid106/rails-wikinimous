
<div class="container text-center">
  <div class="row">
    <div class="col-xs-12">
      <h1>All Articles</h1>
    </div>
  </div>
</div>

<div class="container text-center">
  <div class="row">
    <div class="col-xs-12">
      <% @articles.each do |article| %>
        <h3><%= link_to article.title, article_path(article)  %></h3>
      <% end %>
    </div>
  </div>
</div>




