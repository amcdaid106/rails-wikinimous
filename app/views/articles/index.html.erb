

<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <article>
        <% @articles.each do |article| %>
          <h2><%= link_to article.title, article_path(article) %></h2>
          <%= article.content %>
        <% end %>
      </article>
    </div>
  </div>
</div>



